package com.zeustel.top9.bean;

import android.view.View;

import java.io.Serializable;

public class ScreenShotControl implements Serializable {

    /**
     *
     */
    private static final long serialVersionUID = 1L;

    private int left = 0;
    private int top = 0;
    private int w = 0;
    private int h = 0;
    private View mButton = null;


    public ScreenShotControl(int left, int top, int w, int h, View mButton) {
        super();
        this.left = left;
        this.top = top;
        this.w = w;
        this.h = h;
        this.mButton = mButton;
    }

    public int getLeft() {
        return left;
    }

    public void setLeft(int left) {
        this.left = left;
    }

    public int getTop() {
        return top;
    }

    public void setTop(int top) {
        this.top = top;
    }

    public int getW() {
        return w;
    }

    public void setW(int w) {
        this.w = w;
    }

    public int getH() {
        return h;
    }

    public void setH(int h) {
        this.h = h;
    }

    public View getmButton() {
        return mButton;
    }

    public void setmButton(View mButton) {
        this.mButton = mButton;
    }

}
