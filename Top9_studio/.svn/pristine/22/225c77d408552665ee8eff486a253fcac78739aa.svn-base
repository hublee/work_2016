package com.zeustel.top9.bean;

/**
 *菜单项
 * @author NiuLei
 * @date 2015/8/9 19:50
 */
public class MenuItem {
    //图标
    private int img;
    //文字
    private String text;
    public MenuItem(){

    }
    public MenuItem(int img, String text) {
        this.img = img;
        this.text = text;
    }

    public int getImg() {
        return img;
    }

    public void setImg(int img) {
        this.img = img;
    }

    public String getText() {
        return text;
    }

    public void setText(String text) {
        this.text = text;
    }
}
