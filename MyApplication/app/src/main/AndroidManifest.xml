<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hlb.myapplication" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>

        <activity
            android:name="com.example.BinderActivity"
            android:label="@string/app_name" >

        </activity>


        <service android:name="com.example.service.ExampleService"/>
        <service android:name="com.example.service.BinderService"/>
        <service android:name="com.example.service.MyService"/>
        <service android:name="com.example.service.StatusService"/>
    </application>

</manifest>
